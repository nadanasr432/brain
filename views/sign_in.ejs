      <img  src="img/group2.jpg" >
    </header>
    <section class="container">
     
     <form action="#" class="form">
        <p class="title">Sing In Now </p>
        <div class="input-box">
          <label class="email_address">Email address</label>
          <input type="text" placeholder="Enter email address" required />
        </div>
    
        <div class="input-box">
          <label class="password">password</label>
          <input type="password" placeholder="Enter password" required />
        </div>
        <div class="option_field">
            <span class="checkbox">
              <input type="checkbox" id="check" />
              <label for="check">Remember me</label>
            </span>
            <a href="http://localhost:5000/forgetpassword" class="forgot_pw">Forgot password?</a>
        </div>
     </form>
    </section>
    <div class="dowwn">
       <button class="signin">SIGN IN</button>
       <div class="downsign">
        <h2 >don't have an account?</h2>
        <a href="http://localhost:5000/sign_up" class="c_account"> create account</a>
       </div>
    </div>
    
  </body>
